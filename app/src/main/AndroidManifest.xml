<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ibta.aluno.conversor2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ComprimentoActivity"
            android:label="@string/label_comprimento" />
        <activity
            android:name=".MassaActivity"
            android:label="@string/label_massa" />
        <activity
            android:name=".TemperaturaActivity"
            android:label="@string/label_temperatura" />
        <activity android:name=".CFKActivity"
            android:label="@string/CFK"/>
    </application>

</manifest>